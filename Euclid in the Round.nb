Cell[TextData[{
 "First we define a function for computing the expected inaccuracy of \
probability assignment ",
 StyleBox["q",
  FontSlant->"Italic"],
 ", given probability assignment ",
 StyleBox["p ",
  FontSlant->"Italic"],
 "(and a distance function, Euclidean distance in this case):"
}], "Text",
 CellChangeTimes->{{3.6968585058069487`*^9, 3.696858606808199*^9}, {
  3.696858646619969*^9, 3.6968586625961742`*^9}, {3.696858840643194*^9, 
  3.696858866234243*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"distance", "[", 
    RowBox[{"v1_", ",", "v2_"}], "]"}], ":=", 
   RowBox[{"Sqrt", "[", 
    RowBox[{"Total", "[", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"v1", "-", "v2"}], ")"}], "^", "2"}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"vertices", "[", "p_", "]"}], ":=", 
   RowBox[{"IdentityMatrix", "[", 
    RowBox[{"Length", "[", "p", "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"EI", "[", 
   RowBox[{"p_", ",", "q_"}], "]"}], ":=", 
  RowBox[{"Total", "[", 
   RowBox[{"p", "*", 
    RowBox[{"distance", "[", 
     RowBox[{"q", ",", 
      RowBox[{"vertices", "[", "p", "]"}]}], "]"}]}], "]"}]}]}], "Input",
